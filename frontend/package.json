    "@tailwindcss/vite": "^4.2.0",
    "autoprefixer": "^10.4.24",
    "postcss": "^8.5.6",
    "svelte": "^5.53.3",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^4.2.0",
    "typescript": "^5.3.0",
    "vite": "^7.3.1",
    "vitest": "^1.0.0"
  },
  "type": "module"
